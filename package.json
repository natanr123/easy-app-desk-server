{
  "name": "easy-app-desc-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --watch ./routes ./bin/www",
    "local_start": "node bin/www",
    "docker_build": "docker build -t easy-app-desc-server .",
    "docker_run": "docker run -i --rm -p 8080:3000  -e S3_BUCKET=$S3_BUCKET -e AWS_ACCESS_KEY_ID=$AWS_ACCESS_KEY_ID -e AWS_SECRET_ACCESS_KEY=$AWS_SECRET_ACCESS_KEY -e AWS_REGION=$AWS_REGION --name easy-app-desc-server easy-app-desc-server",
    "docker_stop_container": "docker stop $(docker ps -a -q)",
    "docker_remove_container": "docker rm $(docker ps -a -q)",
    "docker_remove_images": "docker rmi $(docker images -q)"
  },
  "dependencies": {
    "aws-sdk": "^2.657.0",
    "babel-plugin-transform-es2015-modules-commonjs": "*",
    "babel-plugin-transform-runtime": "*",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "*",
    "babel-preset-es2015": "*",
    "babel-preset-es2015-node6": "*",
    "babel-preset-es2016": "*",
    "babel-preset-es2017": "*",
    "babel-preset-flow": "*",
    "babel-preset-stage-0": "*",
    "babel-preset-stage-2": "*",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "mongoose": "^5.9.9",
    "morgan": "~1.9.0",
    "multer": "^1.3.1",
    "nodemon": "^1.18.3",
    "okrand": "^1.0.1",
    "sharp": "^0.25.2 --unsafe-perm"
  }
}
